  "version": "1.0.0",
  "description": "A basic typescript project template, e.g. for code retreats",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "node --loader ts-node/esm --import ./src/instrumentation.ts src/index.ts",
